{
  "name": "angular-suite-flipper",
  "version": "3.1.0",
  "description": "Angular Suite Flipper",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha $(find lib -name \"*.spec.js\") --compilers js:babel-register",
    "build": "rm -rf dist/* && babel lib --ignore *.spec.js --out-dir dist --source-maps",
    "prepublish": "npm test && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/dist/lib/plugin-noop.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/angular-suite-flipper.git"
  },
  "author": "Emartech",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emartech/angular-suite-flipper/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "semantic-release": "4.3.5"
  },
  "homepage": "https://github.com/emartech/angular-suite-flipper",
  "dependencies": {
    "ng1-decorators": "1.0.4",
    "suite-flipper-js": "2.0.1"
  }
}
